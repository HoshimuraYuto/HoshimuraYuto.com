{
  "scripts": {
    "migration:generate": "drizzle-kit generate:sqlite --out migrations --schema src/schema.ts",
    "migration:local": "wrangler d1 migrations apply hoshimurayuto --local",
    "migration:prod": "wrangler d1 migrations apply hoshimurayuto",
    "dev": "lsof -ti :50431 | xargs kill -9 && wrangler dev src/index.ts --port 50431",
    "deploy": "wrangler deploy --minify src/index.ts"
  },
  "dependencies": {
    "hono": "^3.11.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "better-sqlite3": "^9.2.2",
    "drizzle-kit": "^0.20.6",
    "drizzle-orm": "^0.29.1",
    "wrangler": "^3.21.0"
  }
}
