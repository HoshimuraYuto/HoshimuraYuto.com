{
  "name": "hoshimurayuto.com",
  "version": "0.1.0-alpha",
  "private": true,
  "scripts": {
    "dev": "pnpm json:create && PORT=50430 next dev",
    "build:local": "rm -rf {.next,out} && source .env && rm -rf content && git clone $CONTENT_GIT_URL content && pnpm json:blog && next build",
    "build:prod": "NODE_ENV=production pnpm json:create && next build && next-sitemap",
    "build:storybook": "unocss 'app/**/*' --out-file=.storybook/public/uno.css && storybook build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006 & unocss 'app/**/*' --out-file=.storybook/public/uno.css --watch",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:gui": "playwright test --ui",
    "test:storybook": "test-storybook",
    "json:create": "tsx scripts/createJson/index.ts",
    "migration:generate": "drizzle-kit generate:sqlite --out migrations --schema worker/src/schema.ts",
    "migration:local": "wrangler d1 migrations apply hoshimurayuto --local",
    "migration:prod": "wrangler d1 migrations apply hoshimurayuto",
    "worker:dev": "lsof -ti :50431 | xargs kill -9 && wrangler dev worker/src/index.ts --port 50431",
    "worker:deploy": "wrangler deploy --minify worker/src/index.ts"
  },
  "dependencies": {
